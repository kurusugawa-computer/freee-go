{
	"name": "${localWorkspaceFolderBasename}",
	"dockerComposeFile": "compose.yaml",
	"service": "dev",
	"containerUser": "vscode",
	"remoteUser": "vscode",
	"updateRemoteUserUID": true,
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"extensions": [
				"streetsidesoftware.code-spell-checker",
				"jgclark.vscode-todo-highlight",
				"golang.go"
			]
		}
	}
}
